{
   "manifest_version": 2,
   "name": "JenkinsさんからWebsocketを受け取るやつ2",
   "version": "2.0.0",
   "description": "Jenkinsさんからの通知を受け取ります",
   "background": {
       "scripts": [
            "vendor/jquery/jquery.min.js",
            "js/debug.js",
            "js/config.js",
            "js/result.js",
            "js/notification.js",
            "js/main.js"
       ]
   },
   "icons": {
      "128": "img/yusakins.png",
      "16": "img/yusakins.png",
      "48": "img/yusakins.png"
   },
   "options_page": "html/options.html",
   "permissions": [
        "notifications",
        "background"
   ],
   "web_accessible_resources": [
      "img/icon.png",
      "img/icon-failure.png"
   ],
   "default_locale": "en"
}

